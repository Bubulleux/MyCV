*, *:after, *:before {
  box-sizing: inherit; }

* {
  font-family: Verdana, Geneva, Tahoma, sans-serif; }

html > body {
  background-color: #d1b794; }

main {
  margin: 0px 10%; }

main > .home-section {
  overflow: auto; }
  main > .home-section:nth-child(odd) {
    background-color: #ccccccff; }
  main > .home-section:nth-child(even) {
    background-color: #ff0000ff; }
